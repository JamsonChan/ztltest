<test tags="datebox,calendar">
	<case id="constraint">
		<server><!--
			<zk>
			Please selet today, and then it should not appear a warning box.
			<datebox constraint="no future" id="db"/>
			</zk>
		--></server>
		<client><![CDATA[
			click(db.$n("btn"));
			waitResponse();
			click(jq("td.z-calendar-selected"));
			waitResponse();
			blur(db.$n("real"));
			waitResponse();
			verifyFalse(jq(".z-errorbox").exists());
		]]></client>
	</case>
</test>